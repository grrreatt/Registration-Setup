# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Security Configuration
NEXT_PUBLIC_ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com
NEXTAUTH_SECRET=your_nextauth_secret_key
NEXTAUTH_URL=http://localhost:3000

# Rate Limiting (optional - uses in-memory by default)
REDIS_URL=redis://localhost:6379

# Logging (optional)
LOG_LEVEL=info
SENTRY_DSN=your_sentry_dsn

# Email Configuration (optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# QR Code Configuration
QR_CODE_SIZE=256
QR_CODE_MARGIN=2

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/registration_system

# Application Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_NAME=Registration System
NEXT_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
NEXT_PUBLIC_ENABLE_QR_SCANNING=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_EXPORT=true

# Performance Configuration
NEXT_PUBLIC_MAX_FILE_SIZE=10485760
NEXT_PUBLIC_REQUEST_TIMEOUT=30000


